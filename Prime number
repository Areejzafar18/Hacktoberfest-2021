#include<stdio.h>  
int main()
{    
int n,i,m=0,flag=0;    
printf("Enter the number to check weather it is prime: or n o t !\n");    
scanf("%d",&n);    
m=n/2;    
for(i=2;i<=m;i++)    //starting with 2 because 2 is an even prime number
{    
if(n%i==0)    
{    
printf("Number is n o t prime");    
flag=1;    
break;    
}    
}    
if(flag==0)    
printf("Number  i s   prime");     
return 0;  
 }  
